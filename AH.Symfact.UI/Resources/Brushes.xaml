<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="HeaderBackgroundBrush" Color="Orange"/>
    <SolidColorBrush x:Key="HeaderForegroundBrush" Color="White"/>
    <SolidColorBrush x:Key="MenuBackgroundBrush" Color="Red"/>
    <SolidColorBrush x:Key="MenuForegroundBrush" Color="White"/>
    <SolidColorBrush x:Key="MainBackgroundBrush" Color="DarkGreen"/>
    <SolidColorBrush x:Key="MainForegroundBrush" Color="White"/>
    <SolidColorBrush x:Key="StatusBackgroundBrush" Color="Blue"/>
    <SolidColorBrush x:Key="StatusForegroundBrush" Color="White"/>
</ResourceDictionary>
