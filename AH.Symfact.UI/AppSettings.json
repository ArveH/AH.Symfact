{
  "ConnectionStrings": {
    "Symfact": "Server=.;Database=Symfact;TrustServerCertificate=True;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "D:\\Temp\\LogFiles\\Application\\symfact.ui.log",
          "fileSizeLimitBytes": "100000000",
          "rollOnFileSizeLimit": "true",
          "shared": "true",
          "flushToDiskInterval": "00:00:01",
          "outputTemplate": "{Timestamp:yyyyMMdd HH:mm:ss} [{Level:u3}:{ProcessId}] {Message:lj}  {Properties:j} {Exception}{NewLine}"
        }
      }
    ]
  }
}